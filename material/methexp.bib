	month = sep,
	year = {2012},
	keywords = {Associative learning, Classical Conditioning, Comparative cognition, Human learning, Latent inhibition, Neurosciences, Psychology, general, Stimulus preexposure},
	pages = {320--333}
}

@article{natal_generalization_2013,
	title = {Generalization of feature- and rule-based learning in the categorization of dimensional stimuli: {Evidence} for dual processes under cognitive control},
