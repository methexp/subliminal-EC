@article{olivers_boost_2008,
	title = {A boost and bounce theory of temporal attention},
	volume = {115},
	issn = {0033-295X},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2008-14936-012&site=ehost-live},
	doi = {10.1037/a0013395},
	abstract = {What is the time course of visual attention? Attentional blink studies have found that the 2nd of 2 targets is often missed when presented within about 500 ms from the 1st target, resulting in theories about relatively long-lasting capacity limitations or bottlenecks. Earlier studies, however, reported quite the opposite finding: Attention is transiently enhanced, rather than reduced, for several hundreds of milliseconds after a relevant event. The authors present a general theory, as well as a working computational model, that integrate these findings. There is no central role for capacity limitations or bottlenecks. Central is a rapidly responding gating system (or attentional filter) that seeks to enhance relevant and suppress irrelevant information. When items sufficiently match the target description, they elicit transient excitatory feedback activity (a "boost" function), meant to provide access to working memory. However, in the attentional blink task, the distractor after the target is accidentally boosted, resulting in subsequent strong inhibitory feedback response (a "bounce"), which, in effect, closes the gate to working memory. The theory explains many findings that are problematic for limited-capacity accounts, including a new experiment showing that the attentional blink can be postponed. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
	number = {4},
	urldate = {2014-10-29},
	journal = {Psychological Review},
	author = {Olivers, Christian N. L. and Meeter, Martijn},
	year = {2008},
	keywords = {Attention, attentional blink, Awareness, Eyeblink Reflex, Time, time course, Visual Attention},
	pages = {836--863},
	file = {Olivers_etal_2008.pdf:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\6PFHUV7H\\Olivers_etal_2008.pdf:application/pdf}
}

