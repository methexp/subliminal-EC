@article{kusev_judgments_2011,
	title = {Judgments relative to patterns: {How} temporal sequence patterns affect judgments and memory},
	volume = {37},
	issn = {0096-1523},
	shorttitle = {Judgments relative to patterns},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2011-22476-001&site=ehost-live},
	doi = {10.1037/a0025589},
	abstract = {Six experiments studied relative frequency judgment and recall of sequentially presented items drawn from 2 distinct categories (i.e., city and animal). The experiments show that judged frequencies of categories of sequentially encountered stimuli are affected by certain properties of the sequence configuration. We found (a) a first-run effect whereby people overestimated the frequency of a given category when that category was the first repeated category to occur in the sequence and (b) a dissociation between judgments and recall; respondents may judge 1 event more likely than the other and yet recall more instances of the latter. Specifically, the distribution of recalled items does not correspond to the frequency estimates for the event categories, indicating that participants do not make frequency judgments by sampling their memory for individual items as implied by other accounts such as the availability heuristic (Tversky \& Kahneman, 1973) and the availability process model (Hastie \& Park, 1986). We interpret these findings as reflecting the operation of a judgment heuristic sensitive to sequential patterns and offer an account for the relationship between memory and judged frequencies of sequentially encountered stimuli. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
	number = {6},
	urldate = {2016-02-25},
	journal = {Journal of Experimental Psychology: Human Perception and Performance},
	author = {Kusev, Petko and Ayton, Peter and van Schaik, Paul and Tsaneva-Atanasova, Krasimira and Stewart, Neil and Chater, Nick},
	year = {2011},
	keywords = {categorization, Classification (Cognitive Process), frequency judgments, Judgment, Memory, Pattern Discrimination, temporal-sequence patterns},
	pages = {1874--1886},
	file = {Kusev_etal_2011_Judgments_relative_to_patterns.pdf:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\S8PJ9ZBZ\\Kusev_etal_2011_Judgments_relative_to_patterns.pdf:application/pdf}
