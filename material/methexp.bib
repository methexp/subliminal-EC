	pages = {77--93},
	file = {Morís_etal_2014.pdf:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\US49NKW4\\Morís_etal_2014.pdf:application/pdf}
}

@article{gawronski_generalization_2010,
	title = {Generalization versus contextualization in automatic evaluation},
	volume = {139},
	issn = {0096-3445},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2010-20418-001&site=ehost-live},
	doi = {10.1037/a0020315},
	abstract = {Research has shown that automatic evaluations can be highly robust and difficult to change, highly malleable and easy to change, and highly context dependent. We tested a representational account of these disparate findings, which specifies the conditions under which automatic evaluations reflect (a) initially acquired information, (b) subsequently acquired, counterattitudinal information, or (c) a mixture of both. The account postulates that attention to contextual cues during the encoding of evaluative information determines whether this information is stored in a context-free representation or a contextualized representation. To the extent that attention to context cues is low during the encoding of initial information but is enhanced by exposure to expectancy-violating counterattitudinal information, initial experiences are stored in context-free representations, whereas counterattitudinal experiences are stored in contextualized representations. Hence, automatic evaluations tend to reflect the valence of counterattitudinal information only in the context in which this information was learned (occasion setting) and the valence of initial experiences in any other context (renewal effect). Four experiments confirmed these predictions, additionally showing that (a) the impact of initial experiences was reduced for automatic evaluations in novel contexts when context salience during the encoding of initial information was enhanced, (b) context effects were eliminated altogether when context salience during the encoding of counterattitudinal information was reduced, and (c) enhanced context salience during the encoding of counterattitudinal information produced context-dependent automatic evaluations even when there was no contingency between valence and contextual cues. Implications for automatic evaluation, learning theory, and interventions in applied settings are discussed. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
	number = {4},
	urldate = {2015-05-18},
	journal = {Journal of Experimental Psychology: General},
	author = {Gawronski, Bertram and Rydell, Robert J. and Vervliet, Bram and De Houwer, Jan},
	month = nov,
	year = {2010},
	keywords = {automatic evaluation, context effects, Contextual Associations, Cues, Evaluation, evaluative learning, learning, occasion setting, renewal effects},
	pages = {683--701},
	file = {EBSCO Full Text:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\PUCQF7G3\\Gawronski et al. - 2010 - Generalization versus contextualization in automat.pdf:application/pdf}
}

@article{kattner_reconsidering_2014,
	title = {Reconsidering the (in)sensitivity of evaluative conditioning to reinforcement density and {CS}–{US} contingency},
	volume = {45},
	issn = {0023-9690},
	url = {http://www.sciencedirect.com/science/article/pii/S0023969013000349},
	doi = {10.1016/j.lmot.2013.09.002},
	abstract = {Evaluative conditioning (EC) refers to a change in the valence of a stimulus as a result of being paired with an affective stimulus. Based on previous findings, it has been argued that this form of learning does not depend on a statistical relation between the CS and the US. However, the empirical evidence on this issue is still rather limited and based mainly on null-effects. The present series of experiments tested the reliability of EC effects with different types of CS–US relations. The results of Experiment 1 show that unpaired CS presentations during conditioning (i.e., modulations of reinforcement density) do not interfere with the acquisition of EC, whereas the magnitude of the EC effect depended on the total number of contiguous CS–US presentations. Experiment 2 demonstrated that EC does not depend on a predictive relation between the CS and the US (contingency), either. Reliable EC effects were shown even with CSs that predicted the absence of a US (negative contingency). Throughout the experiments, EC was restricted to those contiguities that were memorized by participants. Subjective judgments of the contingency relation accounted for an additional indirect effect on EC. Taken together, the results suggest that EC depends on neither reinforcement density nor contingency, whereas propositions containing contingency information might exert some influence.},
	urldate = {2015-11-24},
	journal = {Learning and Motivation},
	author = {Kattner, Florian},
	month = feb,
	year = {2014},
	keywords = {Contiguity, Contiguity memory, Contingency, Contingency judgments, Evaluative conditioning, Reinforcement density, Valence awareness},
	pages = {15--29},
	file = {Kattner_2014.pdf:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\X8HMZBDV\\Kattner_2014.pdf:application/pdf}
}

