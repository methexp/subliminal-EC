	keywords = {Associative Processes, Associative learning, Cognitive Hypothesis Testing, Generalization (Learning), Human Channel Capacity, Rules, Short Term Memory, concurrent load, contingency learning, deliberative processing, feature-based, generalization, learning, rule-based, working memory},
	pages = {308--316}
}

@article{livesey_attentional_2009,
	title = {Attentional changes during implicit learning: {Signal} validity protects a target stimulus from the attentional blink},
