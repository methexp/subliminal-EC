	keywords = {Cognitive Processes, Implicit Memory, Information Processing Model, Memory, Models, Practice, Recall (Learning), Response Bias, adults, information processing model as explanation for response bias \& repetition priming effects in implicit memory tasks, priming},
	pages = {403--421}
}

@article{de_houwer_association_2001,
	title = {Association learning of likes and dislikes: {A} review of 25 years of research on human evaluative conditioning},
