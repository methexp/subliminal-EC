	pages = {577--595},
	file = {GM2014CEM.pdf:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\46GMEVZ7\\GM2014CEM.pdf:application/pdf}
}

@article{dechene_mix_2009,
