}

@article{ulrich_flexible_2014,
	title = {Flexible establishment of functional brain networks supports attentional modulation of unconscious cognition.},
	volume = {Vol.35},
	copyright = {© 2014 Wiley Periodicals, Inc},
	url = {http://www.ncbi.nlm.nih.gov/m/pubmed/24954512/},
	doi = {10.1002/hbm.22566},
	abstract = {In classical theories of attention, unconscious automatic processes are thought to be independent of higher-level attentional influences. Here, we propose that unconscious processing depends on attentional enhancement of task-congruent processing pathways implemented by a dynamic modulation of the functional communication between brain regions. Using functional magnetic resonance imaging, we tested our model with a subliminally primed lexical decision task preceded by an induction task preparing either a semantic or a perceptual task set. Subliminal semantic priming was significantly greater after semantic compared to perceptual induction in ventral occipito-temporal (vOT) and inferior frontal cortex, brain areas known to be involved in semantic processing. The functional connectivity pattern of vOT varied depending on the induction task and successfully predicted the magnitude of behavioral and neural priming. Together, these findings support the proposal that dynamic establishment of functional networks by task sets is an important mechanism in the attentional control of unconscious processing.},
	language = {Eng},
	urldate = {2015-09-02},
	journal = {Human Brain Mapping},
	author = {Ulrich, M and Adams, SC and Kiefer, M},
	month = jun,
	year = {2014},
	file = {Ulrich et al. - 2014 - Flexible establishment of functional brain network.pdf:C\:\\Users\\stahlc\\Dropbox\\zotero\\storage\\AIS6PGR3\\Ulrich et al. - 2014 - Flexible establishment of functional brain network.pdf:application/pdf}
}

