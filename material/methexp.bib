	author = {Hofmann, Wilhelm and De Houwer, Jan and Perugini, Marco and Baeyens, Frank and Crombez, Geert},
	year = {2010},
	keywords = {\_tablet\_modified, *Classical Conditioning, *Learning, affective learning, associative learning, attitude learning, Conditioned Stimulus, evaluative conditioning, propositional, Unconditioned Stimulus},
